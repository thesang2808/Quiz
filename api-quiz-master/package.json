{
  "name": "api-quiz",
  "version": "0.0.1",
  "description": "Backend for Quiz",
  "author": "CanhDominich",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "prebuild": "rimraf dist",
    "format": "pretty-quick --pattern \"**/*.*(ts|tsx|json)\" --staged",
    "format:all": "prettier --write ./src/*",
    "build": "TZ=Asia/Ho_Chi_Minh nest build",
    "start": "TZ=Asia/Ho_Chi_Minh nest start",
    "start:dev": "TZ=Asia/Ho_Chi_Minh nest start --watch",
    "start:debug": "TZ=Asia/Ho_Chi_Minh nest start --debug --watch",
    "start:prod": "TZ=Asia/Ho_Chi_Minh node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "node dist/seeder",
    "seed:refresh": "node dist/seeder --refresh",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@golevelup/nestjs-discovery": "^3.0.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/terminus": "^9.2.2",
    "@types/cache-manager": "^4.0.2",
    "acd-util-help": "^1.0.5",
    "bcrypt": "^5.1.0",
    "cache-manager": "^2.10.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "^3.3.8",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express-http-context": "^1.2.4",
    "google-auth-library": "^8.7.0",
    "handlebars": "^4.7.7",
    "husky": "^8.0.3",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.40",
    "mongoose": "^6.8.0",
    "nestjs-seeder": "^0.3.0",
    "nodemailer": "^6.9.1",
    "npm": "^9.6.3",
    "pm2": "^5.3.0",
    "pretty-quick": "^3.1.3",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "utf8": "^3.0.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.11",
    "jest": "28.1.3",
    "prettier": "^2.8.7",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
